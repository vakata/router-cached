{
    "name": "vakata/routerCached",
    "description": "A simple request router with cached",
    "keywords": [
        "vakata",
        "routerCached"
    ],
    "homepage": "https://github.com/vakata/routerCached",
    "license": "MIT",
    "authors": [
        {
            "name": "vakata",
            "email": "github@vakata.com",
            "homepage": "http://www.vakata.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "vakata/router" : "1.*",
        "vakata/cache" : "1.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "vakata\\routerCached\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "vakata\\routerCached\\test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "psr2": "php-cs-fixer fix ./src",
        "phpdoc": "phpDocumentor -d ./src -t ./docs"
    }
}
